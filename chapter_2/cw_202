(
SELECT FIRST_NAME, LAST_NAME, 'Klient' AS GUEST_TYPE
	FROM CUSTOMERS
	WHERE CITY = 'Los Angeles' AND STATE = 'CA'
)
UNION
(
SELECT FIRST_NAME, LAST_NAME, 'Pracownik' AS GUEST_TYPE
	FROM SALESPEOPLE S
	INNER JOIN DEALERSHIPS D ON D.DEALERSHIP_ID = S.DEALERSHIP_ID
	WHERE D.CITY = 'Los Angeles' AND STATE = 'CA'
)